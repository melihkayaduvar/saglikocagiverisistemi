   <string notr="true">/* Hastane Teması QSS */
QMainWindow#anapencere{
	border-image: url(:/new/background/main-background)
}
QWidget {
    font-family: Arial, sans-serif;
    color: #333; /* Metin rengi */
    border-radius: 5px;
    padding: 10px;
}

QPushButton {
    background-color: #4CAF50; /* Yeşil buton */
    color: white;
    border-radius: 5px;
    padding: 10px 20px;
    font-size: 16px;
    border: none;
}

QPushButton:hover {
    background-color: #45a049; /* Hover durumunda daha koyu yeşil */
}

QPushButton:pressed {
    background-color: #388e3c; /* Tıklanma sonrası yeşil */
}

QLineEdit {
    background-color: #ffffff;
    border: 1px solid #ccc;
    padding: 8px;
    border-radius: 5px;
    font-size: 14px;
}

QLineEdit:focus {
    border-color: #4CAF50; /* Odaklandığında yeşil sınır */
}

QLabel {
    font-size: 18px;
    color: #333;
    margin-bottom: 5px;
}

QComboBox {
    background-color: #ffffff;
    border: 1px solid #ccc;
    border-radius: 5px;
    padding: 5px;
    font-size: 14px;
}

QComboBox:hover {
    border-color: #4CAF50; /* Hoverda yeşil sınır */
}

QTabWidget {
    background-color: rgba(0, 0, 0, 0); /* Şeffaf arka plan */
    border: none;
}

QTabWidget::pane {
    background-color: transparent;
    border: none;
}
/* Sekme çubuğu */
QTabBar::tab {
	background-color: rgba(43, 76, 147,0.2);
    color: white;
    font-family: &quot;Arial Black&quot;;
    font-size: 10px;
    font-weight: bold;
    padding: 8px 15px;
    border: 1px solid white;
    border-radius: 4px;
    margin: 2px;
}

/* Seçili sekme */
QTabBar::tab:selected {
    background-color: rgba(43, 76, 147,0.6);
    color: white;
    border: 1px solid rgb(43, 76, 147);
}

/* Hover (fareyle üzerine gelindiğinde) */
QTabBar::tab:hover {
    background-color: rgba(29, 132, 132,0.6);
    color: white;
    border: 1px solid rgba(29, 132, 132,0.6);
}

QScrollBar:horizontal, QScrollBar:vertical {
    background: #f0f8ff;
    border: 1px solid #ccc;
    border-radius: 5px;
    height: 8px;
    width: 8px;
}

QScrollBar::handle:horizontal, QScrollBar::handle:vertical {
    background: #4CAF50;
    border-radius: 5px;
}

QScrollBar::handle:horizontal:hover, QScrollBar::handle:vertical:hover {
    background: #388e3c;
}
/* Menü çubuğu arka plan rengi ve sınırları */
QMenuBar {
    background-color:rgba(0, 0, 0, 0.2); 
    color: white; /* Yazı rengi beyaz */
	font-family: &quot;Arial Black&quot;;
    font-size: 10px; /* Font boyutu */
    font-weight: bold;
    border: 1px; /* Kenarlık yok */
    padding: 2px;
}

/* Menü öğeleri */
QMenuBar::item {
    background-color: transparent; /* Arka planı şeffaf */
    color: white; /* Yazı rengi beyaz */
    padding: 8px 15px; /* İç boşluk */
}

/* Menü öğeleri üzerine gelindiğinde renk değişimi */
QMenuBar::item:selected {
    background-color: rgba(43, 76, 147,0.6);
    color: white;
}

/* Menü öğeleri üzerine gelindiğinde fareyle etkileşim */
QMenuBar::item:hover {
    background-color: rgba(29, 132, 132,0.6);
    color: white;
    border: 1px solid rgba(29, 132, 132,0.6);
}

/* Menü arka planı */
QMenu {
    background-color:rgba(21, 24, 62, 0.4);  /* Koyu gri arka plan */
    border: 1px solid #555; /* Gri kenarlık */
    color: white; /* Menü öğesi yazı rengi beyaz */
    padding: 5px;
}

/* Menü öğelerinin üzerine gelindiğinde değişiklik */
QMenu::item {
    padding: 8px 15px; /* Menü öğesi iç boşluğu */
    background-color: transparent; /* Arka planı şeffaf */
}

QMenu::item:selected {
    background-color: rgba(43, 76, 147,0.6);
    color: white;
}

/* Menü öğeleri üzerine gelindiğinde */
QMenu::item:hover {
    background-color: rgba(29, 132, 132,0.6);
    color: white;
    border: 1px solid rgba(29, 132, 132,0.6);
}
</string>
